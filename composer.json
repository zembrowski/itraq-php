{
    "name": "zembrowski/itraq-php",
    "description": "PHP wrapper for the iTraq API",
    "keywords": [ "itraq", "php", "api", "wrapper"],
    "homepage": "https://github.com/zembrowski/itraq-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Krzysztof Tomasz Zembrowski",
            "homepage": "http://krzysztof.zembrowski.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3",
        "guzzlehttp/guzzle": "~6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.2"
    },
    "autoload": {
        "psr-4": {
            "iTraq\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iTraq\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
